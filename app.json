{
  "expo": {
    "name": "UTS Portal",
    "slug": "uts-portal",
    "privacy": "unlisted",
    "sdkVersion": "35.0.0",
    "androidStatusBar" : {
      "barStyle":"dark-content"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "primaryColor": "#3b79f5",
    "githubUrl": "https://github.com/dkantereivin/uts-portal-react",
    "splash": {
      "image": "./assets/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "buildNumber" : "3",
      "bundleIdentifier": "com.studentdev.utsportal",
      "infoPlist" : {
        "UIBackgroundModes" : [
          "location",
          "fetch"
        ],
        "ITSAppUsesNonExemptEncryption" : false
      }
    },
    "android": {
      "package": "com.utschools.utsportal",
      "versionCode": 3,
      "permissions": []
    }
  }
}